doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    title Admin Uploading - GatherX
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css')
  body
    h1 Event Registrations
    table
      thead
        tr
          th Event Title
          th Description
          th Date & Time
          th Location
          th Registrations
      tbody
        each event in events
          tr
            td= event.title
            td= event.description
            td= event.dateTime.toISOString().substring(0, 10) 
            td= event.location
            td= event.registrationCount 
